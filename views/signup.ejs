<style>
.wizard-card {
    margin-left: 300px;
    margin-top: -50px;
}

.wizard-card {
  -moz-box-shadow:    inset 0 0 10px #000000;
  -webkit-box-shadow: inset 0 0 10px #000000;
  box-shadow:         inset 0 0 10px #000000;
  width: 550px;
  height: 750px;
}

.form-group {
    margin-bottom: 20px;
}

.form-controll {
    padding: 10px 20px;
    margin: 8px 0;
    display: inline-block;
    border: 1px solid #ccc;
    border-radius: 5px;
    box-sizing: border-box;
    -webkit-transition: all 0.3s ease-in-out;
    width: 150px;
}

.wizard-card[data-color="green"] .form-group .form-control {
    background-image: linear-gradient(#fbb710, #fbb710), linear-gradient(#D2D2D2, #D2D2D2);
}


.form-group {
    margin-bottom: -20px;
}

.card {
    background-color: #FFFFFF;
    padding: 0px 0;
  }

  /*nav bar*/

  body {
    font-family: "Lato", sans-serif;
  }

  .sidenav {
    height: 100%;
    width: 0;
    position: fixed;
    z-index: 1;
    top: 100px;
    left: 0;
    background-color: #fbb710;
    overflow-x: hidden;
    transition: 0.5s;
    padding-top: 100px;
    margin-top: -100PX;
    height: 100%;

  }

  .sidenav a {
    padding: 5px;
    text-decoration: none;
    color: white;
    display: block;
    transition: 0.3s;
    font-size: 15px;
    font-weight: 900;
  }

  .sidenav a:hover {
    color: black;
  }

  .sidenav .closebtn {
    position: absolute;
    top: 0;
    right: 25px;
    font-size: 36px;
    margin-left: 50px;
  }

  @media screen and (max-height: 450px) {
    .sidenav {padding-top: 15px;}
    .sidenav a {font-size: 18px;}
  }

  .nbutton1 {
    margin-left: 10%;
    background-color: black;
    color: black;
    border: 2px solid #fbb710;
    text-align:center;
    width : 80%;
    border-radius: 10px;
    vertical-align: middle;
    text-align:center;
    padding: 5px;
  }

  .nbutton1:hover {
    background-color: white;
    color: white;
  }

  /*NAV BAR*/
  .input, .textarea {
    width: 97%;
    padding: 5px;
    border: 1px solid rgba(0,0,0,.1);
    border-radius: 8px;
  }
  .input:focus, .textarea:focus{
    outline: 0;
    box-shadow: 0 0 3px #5FA7D3;
    border: 1px solid #5FA7D3;
  }


  .input[type='submit'] {
    background: #fbb710;
    box-shadow: 4 0px 0 #448FBD;
    color: #black;
    font-weight: bold;
    width: 150px;
    height: 30px;
    border-radius: 15px solid #5FA7D3;
  }
  .input[type='submit']:focus {
    box-shadow: none;
    cursor: pointer;
  }

/*cart clear cart*/
.button {
padding: 5px 15px;
background-color: #fbb710; /* Green */
border-radius:5px;
color: black;
text-align: center;
display: inline-block;
font-size: 20px;
margin: 10px;
transition-duration: 0.2s;
cursor: pointer;
margin-right: 75px;
}

.button1 {
background-color: #black;
